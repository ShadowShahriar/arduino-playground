// === 3.5 Inch TFT 320×480 ===
/* === 8-bit LCD ===
 * TFT Shield     Arduino Nano
 * LCD_RST    --> A4
 * LCD_CS     --> A3
 * LCD_RS     --> A2
 * LCD_WR     --> A1
 * LCD_RD     --> A0
 * LCD_D0     --> GPIO 8
 * LCD_D1     --> GPIO 9
 * LCD_D2     --> GPIO 2
 * LCD_D3     --> GPIO 3
 * LCD_D4     --> GPIO 4
 * LCD_D5     --> GPIO 5
 * LCD_D6     --> GPIO 6
 * LCD_D7     --> GPIO 7
 * GND        --> GND
 * 5V         --> 5V
 */

#define LANDSCAPE false
#define FLIP false
#define TFT_MS 250
MCUFRIEND_kbv tft;

void background(uint16_t color)
{
	tft.fillScreen(color);
}

uint16_t rgb(uint8_t r, uint8_t g, uint8_t b)
{
	return tft.color565(r, g, b);
}

void clearDisplay()
{
	tft.fillScreen(0x0000);
}

void setupTFT()
{
	uint16_t ID = tft.readID();
	if (ID == 0xD3D3)
		ID = 0x9481;
	delay(TFT_MS);
	tft.begin(ID);

	int rotate = 0;
	if (LANDSCAPE && FLIP)
		rotate = 3;
	else if (FLIP)
		rotate = 2;
	else if (LANDSCAPE)
		rotate = 1;
	tft.setRotation(rotate);

	clearDisplay();
	Serial.println(F("✅ TFT OK"));
}

// === === === === === === === === === === ===

void setupSerial()
{
	Serial.begin(9600);
	for (int i = 0; i < 10; i++)
	{
		delay(100);
		Serial.println(F("."));
	}
	Serial.println(F("✅ Serial OK"));
}

